@using itransition_project.Models
@model IEnumerable<Comix>
<div class="container">
    <!-- Page Heading -->
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">
                 @Model.Count() постов найдено
            </h1>
        </div>
    </div>
    <!-- /.row -->
    <!-- Project One -->
    @{ foreach (Comix en in @Model)
        {
            <div class="row">
                <div class="col-md-7">
                </div>
                <div class="col-md-5">
                    <h3>@en.Name</h3>
                    <h4 class="label label-info">@en.Author</h4>
                </div>
            </div>
            <hr>
        }
    }
</div>